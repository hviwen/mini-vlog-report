{
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "declarationDir": "./dist",
    "target": "es5",
    "lib": ["es2015","dom"],
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": ["./src"],
  "files": [
    "src/index.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
